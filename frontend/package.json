{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"packageManager": "pnpm@8.15.1",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"pretest": "playwright install",
		"test": "playwright test",
		"test-flaky": "playwright test --retries=3 -j 30%",
		"test-hard": "playwright test --repeat-each=3 -j 30%",
		"test-report": "playwright show-report test-results/_html-report",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "svelte-kit sync && eslint .",
		"lint:report": "svelte-kit sync && eslint . --output-file eslint_report.json --format json",
		"format": "eslint . --fix",
		"clean": "rimraf node_modules .svelte-kit"
	},
	"workspaces": [
		"viewer"
	],
	"devDependencies": {
		"@egoist/tailwindcss-icons": "^1.7.4",
		"@eslint/js": "^8.56.0",
		"@graphql-codegen/cli": "^5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@graphql-codegen/schema-ast": "^4.0.0",
		"@graphql-codegen/typescript": "^4.0.1",
		"@graphql-typed-document-node/core": "^3.2.0",
		"@iconify-json/mdi": "^1.1.64",
		"@playwright/test": "^1.41.1",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/mjml": "^4.7.4",
		"@types/node": "^20.11.16",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@urql/core": "^4.2.3",
		"@urql/devtools": "^2.0.3",
		"@urql/exchange-graphcache": "^6.4.0",
		"@urql/svelte": "^4.0.4",
		"autoprefixer": "^10.4.17",
		"daisyui": "^4.7.2",
		"eslint": "^8.56.0",
		"eslint-plugin-svelte": "^2.35.1",
		"globals": "^13.24.0",
		"graphql": "^16.8.1",
		"jwt-decode": "^4.0.0",
		"postcss": "^8.4.33",
		"rimraf": "^5.0.5",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"svelte-eslint-parser": "^0.33.1",
		"svelte-preprocess": "^5.1.3",
		"svelte-turnstile": "^0.5.0",
		"sveltekit-superforms": "^1.13.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"type-fest": "^4.10.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-graphql-codegen": "^3.3.6",
		"vitest": "^1.2.2",
        "viewer": "workspace:*",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.6.1",
		"@opentelemetry/api": "^1.7.0",
		"@opentelemetry/auto-instrumentations-node": "^0.41.0",
		"@opentelemetry/auto-instrumentations-web": "^0.36.0",
		"@opentelemetry/context-zone": "^1.21.0",
