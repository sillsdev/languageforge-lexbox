    "postcss": "catalog:",
    "rimraf": "^5.0.10",
    "svelte": "catalog:",
    "svelte-check": "catalog:",
    "svelte-eslint-parser": "catalog:",
    "svelte-preprocess": "catalog:",
    "svelte-turnstile": "^0.9.0",
    "sveltekit-superforms": "^1.13.4",
