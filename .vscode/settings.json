{
    "iconify.includes": [
        "mdi"
    ],
    "i18n-ally.enabledFrameworks": [
        "svelte"
    ],
    "i18n-ally.localesPaths": [
        "frontend/src/lib/i18n/locales"
    ],
    "i18n-ally.keepFulfilled": true,
    "i18n-ally.sortKeys": true,
    "i18n-ally.keystyle": "nested",
    "i18n-ally.tabStyle": "tab",
    "eslint.experimental.useFlatConfig": true,
    "eslint.workingDirectories": [
        { "directory": "./frontend" }
    ],
    "sort-imports.on-save": true,
    "editor.detectIndentation": false,
    "eslint.validate": [
      "svelte"
    ],
    "yaml.schemas": {
        "https://json.schemastore.org/github-workflow.json": ".github/**/*.yaml"
    },
    "files.associations": {
      "**/frontend/**/i18n/**/*.json": "json5", // Supports json5 syntax: https://svelte-intl-precompile.com/en/docs/getting-started
    }
}
