{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConfig": {
    "LexBoxConnectionString": null
  },
  "ReverseProxy": {
    "Routes": {
      "hg": {
        "ClusterId": "hg-web",
        "AuthorizationPolicy": "HgAuth",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "hg-web": {
        "Destinations": {
          "hg-web-server": {
            "Address": "https://hg-public.languageforge.org/"
          }
        }
      }
    }
  }
}
