{
  "ReverseProxy": {
    //config reference https://microsoft.github.io/reverse-proxy/articles/config-files.html#configuration-structure
    "Routes": {
      "resumable": {
        "Order": 10,
        "ClusterId": "resumableWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/api/v03/{**catch-all}"
        }
      },
      "hg-root": {
        "Order": 21,
        "ClusterId": "hgWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/{project-code}/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPrefix": "/hg"
          }
        ]
      },
      "hg": {
        "Order": 20,
        "ClusterId": "hgWeb",
        "AuthorizationPolicy": "UserHasAccessToProject",
        "Match": {
          "Path": "/hg/{project-code}/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "hgWeb": {
        "Destinations": {
          "hgWebServer": {
            "Address": "http://localhost:8088/"
          }
        }
      },
      "resumableWeb": {
        "Destinations": {
          "resumableWebServer": {
            "Address": "http://localhost:8034/"
          }
        }
      }
    }
  },
  "LexBoxApi": {
    "Url": null
  }
}
