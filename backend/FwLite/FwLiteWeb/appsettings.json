{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  // by default a random port is chosen, you can set it here to use a specific port
  // "http_ports": "49279",
  "FwLiteWeb": {
    "LogFileName": null,
    "CorsAllowAny": false,
    "OpenBrowser": true
  },
  "LcmCrdt": {
    // uncomment the following line to set the path where sqlite files are loaded and saved.
    // not used in windows, use BaseDataPath above instead
    //"ProjectPath": ""
  },
  "FwDataBridge": {
    // uncomment the following line to set the path fwdata projects are loaded
    // by default we load them from the ProgramData folder where FW stores it's projects
//    "ProjectsFolder": ""
  }
}
