{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "FwLiteShared.Auth.LoggerAdapter": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.AspNetCore.Components.Server.Circuits.RemoteJSRuntime": "Debug"
    }
  },
  "FwLiteWeb": {
    "CorsAllowAny": false,
    "OpenBrowser": false,
    "LogFileName": "./fw-lite-web.log"
  },
  "FwLite": {
    "UpdateCheckCondition": "Never",
    "UseDevAssets": true
  }
}
